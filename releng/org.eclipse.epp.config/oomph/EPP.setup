      <predicate
          xsi:type="predicates:AndPredicate">
        <operand
            xsi:type="predicates:RepositoryPredicate"
            project="org.eclipse.epp.package.common"/>
        <operand
            xsi:type="predicates:NamePredicate"
            pattern=".*\.jee.*"/>
      </predicate>
    </workingSet>
    <workingSet
        name="EPP Modeling">
      <predicate
          xsi:type="predicates:AndPredicate">
        <operand
            xsi:type="predicates:RepositoryPredicate"
            project="org.eclipse.epp.package.common"/>
        <operand
            xsi:type="predicates:NamePredicate"
            pattern=".*\.modeling.*"/>
      </predicate>
    </workingSet>
    <workingSet
